<!DOCTYPE html>
<html ng-app ="books">
<head>
    <title>Books</title>
</head>
<body>
    
    <div ng-controller="BooksListCtrl">
        <div id="grid1" ui-grid="gridOpts" ui-grid-edit ui-grid-row-edit ui-grid-cellNav class="grid"></div>

        
        <button type="button" id="addData" ng-click="addData()">Add Book</button>
        <button  type="button"  ng-click="save()">Save</button>
    </div>

    <script src="http://ajax.googleapis.com/ajax/libs/angularjs/1.4.3/angular.js"></script>
    <script src="http://ui-grid.info/release/ui-grid.js"></script>
    <link rel="stylesheet" href="http://ui-grid.info/release/ui-grid.css" type="text/css">
    <script src="https://code.jquery.com/jquery-3.0.0.js"
            integrity="sha256-jrPLZ+8vDxt2FnE1zvZXCkCcebI/C8Dt5xyaQBjxQIo=" crossorigin="anonymous"></script>

    @Scripts.Render("~/bundles/Books")
    @Styles.Render("~/Content/css")
</body>
</html> 